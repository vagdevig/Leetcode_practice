class Solution {
    public int[] twoSum(int[] nums, int target) {
        int[] ans = new int[2];
        int i=0;
        while(i < nums.length){
            int check = target-nums[i];
            for(int j=i+1;j<nums.length;j++){
                if(check == nums[j]){
                    ans[0]=i;
                    ans[1]=j;
                }
            }
            i++;
        }
        return ans;
    }
}
